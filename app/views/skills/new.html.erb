<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-sm-6 col-sm-offset-3">
        <%= simple_form_for @skill do |f| %>
          <%= f.input :name, placeholder: "Skill" %>
          <%= f.input :description %>
          <%= f.input :meeting_point, placeholder: "What is your favorite Coffee place?" %>
          <%#= f.input :zip_code %>
          <%#= f.input :city %>
          <%#= f.input :country, priority: [ 'FR' ] %>
          <%= f.input :price %>
          <%= f.button :submit %>
        <% end %>
      </div>
    </div>
  </div>
</div>
